{
  "preset": "google",
  "esnext": true,

  "validateQuoteMarks": "\"",
  "requireParenthesesAroundIIFE": true,

  "disallowMultipleLineBreaks": null,
  "disallowSpacesInsideObjectBrackets": null,
  "maximumLineLength": {
    "value": 100,
    "allExcept": ["comments", "regex"]
  },
  "requireCamelCaseOrUpperCaseIdentifiers": null
}
